{
  "firstName": {
    "label": "First Name",
    "type": "text",
    "placeholder": "Enter your first name",
    "defaultValue": "",
    "rules": {
      "required": true,
      "maxLength": 3
    }
  },
  "lastName": {
    "label": "Last Name",
    "type": "text",
    "placeholder": "Enter your last name",
    "defaultValue": "",
    "rules": {
      "required": true
    }
  },
  "gender": {
    "label": "Gender",
    "type": "radio",
    "options": ["male", "female"],
    "defaultValue": "",
    "rules": {
      "required": true
    }
  },
  "profession": {
    "label": "Profession",
    "type": "dropdown",
    "options": ["Front-end Developer", "Back-end Developer", "Devops Engineer"],
    "defaultValue": "",
    "rules": {
      "required": true
    }
  },
  "agree": {
    "type": "checkbox",
    "label": "Agree",
    "checkboxLabel": "I hereby agree to the terms.",
    "defaultValue": "false",
    "rules": {
      "required": true
    }
  }
}
